# Конфигурация для тестирования с mock-сервером прокси-провайдера

# Отключаем GIGACHAT_AUTH_KEY (не требуется для прокси-моделей)
# GIGACHAT_AUTH_KEY не установлен

# Отключаем установку сертификатов (не нужно для локального тестирования)
INSTALL_RUSSIAN_CERTS=false

# Настройки прокси-провайдера (mock-сервер)
PROXY_PROVIDER_ENABLED=true
PROXY_PROVIDER_URL=http://localhost:8000/v1
PROXY_PROVIDER_AUTH_HEADER=X-Client-Id
PROXY_PROVIDER_AUTH_VALUE=test-client-id-12345
PROXY_PROVIDER_MODEL_SUFFIX=proxy

# Автоматическая синхронизация моделей
MODEL_SYNC_ENABLED=true
MODEL_SYNC_INTERVAL=10  # Короткий интервал для тестирования (10 секунд)

# Таймаут для запросов
GIGACHAT_TIMEOUT=30
